<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.modu.app.prj.login.mapper.LoginMapper">
 
 	<select id="login" resultType="LoginVO" parameterType="LoginVO">
		select *
		from memb
		where id= #{id} 
		and pwd = #{pwd}
	</select>
 	
 	<insert id="member" parameterType="LoginVO">
		insert into memb values
		(concat('m',memb_seq.nextval),
		#{id},
		#{pwd},
		#{nm},
		#{phNo},
		'N',
		'Y',
		'A',
		sysdate)
 	</insert>
 	
 </mapper>
  
  